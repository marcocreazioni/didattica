<body>
    <script>
    // Creating arrays
                  
    var colors = ["Red", "Yellow", "Green", "Orange"];
    var cities = ["London", "Paris", "New York"];
      
     var abbinamenti = colors[1] + " " + cities[2];
     var abbinamentodue = colors[0] + " " + cities[1];
      
    // Printing array values
    document.write(colors[1] + "<br>");   // Output: Red
    document.write(cities[2] + "<br>");   // Output: New York
    document.write(abbinamenti + "<br>");  
   document.write(abbinamentodue);  
    </script>
</body>
