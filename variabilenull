<body>
<!--esempio dell'uso della variabile null e prompt assegnato ad una variabile -->
    <script>
     var casa = 2;
     var villa = 3; 
     var somma = casa + villa;
     
     var casa = null;
     var casa = prompt("valore interesse al 10%");
    
     var interessedue = casa*0.10;
     var casa = null;
     var casa = prompt("valore interesse al 20%");
     var interesse = casa *0.20;
    
    document.write(somma + "<br>"); 
    document.write(interessedue + "<br>");
    document.write(interesse);
    
    
    </script>
</body>
